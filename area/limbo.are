endwhile
// koniec symulacji p`etli for

// dalej powa`zny prog, biega po li`scie postaci w pomieszczeniu i zapami`etuje
// ich imiona.
chr dupa = get_first_char_room( )
str imiona = "Postacie w moim pomieszczeniu: "
int ilu = 0
// poni`zej nie mo`ze by`c deleted( dupa ) jak w przypadku $n/$r/$t, bo
// kiedy ostatni get_next_char_room( ) si`e nie uda, zmienna zniknie,
// a nie przyjmie pustej warto`sci
while varexists( "dupa" )
  if cansee( dupa )
    // dzielenie na kilka linijek je`sli w pomieszczeniu jest kilka mob`ow
    if strlen( imiona ) > 250
      '%(imiona),
      str imiona = ""
    else
      if ilu
        str imiona += ", "
      endif
    endif
    str imiona += name_expand( dupa )
    if ispc( dupa )
      str imiona += " (gracz)"
    else
      str imiona += " (mob)"
    endif
    int ilu = ilu + 1
  else
    mpechoat dupa zawsze $I wie o tobie, ale nie wyda twojej obecno`sci.
  endif
  chr dupa = get_next_char_room( dupa )
endwhile
str imiona += " (razem "
str imiona += itopl( ilu, 2 )
int l = liczba( ilu )
if l == 1
  str imiona += " posta`c)"
else
  if l == 2
    str imiona += " postacie)"
  else
    str imiona += " postaci)"
  endif
endif
'%(imiona)
endif
